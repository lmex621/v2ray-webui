{
  "inbounds":[
    {
      "port":1081,
      "listen":"0.0.0.0",
      "protocol":"socks",
      "settings":{
        "udp":true
      }
    },
    {
      "port":1082,
      "protocol":"dokodemo-door",
      "settings":{
        "network":"tcp,udp",
        "followRedirect":true,
        "timeout":30
      }
    },
    {
      "port":1083,
      "protocol":"http",
      "allowTransparent":false,
      "userLevel":0
    }
  ],
  "log":{
    "error":"v2ray_error.log",
    "loglevel":"warning",
    "access":"v2ray_access.log"
  },
  "outbounds":[
    {
      "mux":{
        "enabled":false,
        "concurrency":8
      },
      "protocol":"trojan",
      "streamSettings":{
        "network":"tcp",
        "tcpSettings":{
          "header":{
            "type":"none"
          }
        },
        "security":"none"
      },
      "tag":"proxy",
      "settings":{
        "servers":[
          {
            "password":"{serverPass}",
            "port":9495945,
            "email":"",
            "level":0,
            "address":"{serverHost}"
          }
        ]
      }
    },
    {
      "tag":"direct",
      "protocol":"freedom",
      "settings":{
        "domainStrategy":"UseIP",
        "userLevel":0
      }
    },
    {
      "tag":"block",
      "protocol":"blackhole",
      "settings":{
        "response":{
          "type":"none"
        }
      }
    }
  ],
  "routing":{
    "domainStrategy":"IPOnDemand",
    "rules":[
      {
        "type":"field",
        "ip":[
          "geoip:private"
        ],
        "outboundTag":"direct"
      }
    ]
  }
}
